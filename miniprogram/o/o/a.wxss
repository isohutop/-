.x{width:99%;text-align:center;font-size:3.4vw;color:#949494;letter-spacing:0.5vw}
.x view {display: inline-block}
.x view:nth-of-type(2){animation-delay:.05s}
.x view:nth-of-type(3){animation-delay:.1s}
.x view:nth-of-type(4){animation-delay:.15s}
.x view:nth-of-type(5){animation-delay:.2s}
.x view:nth-of-type(6){animation-delay:.25s}
.x view:nth-of-type(7){animation-delay:.3s}
.x view:nth-of-type(8){animation-delay:.35s}
.x view:nth-of-type(9){animation-delay:.4s}
.x view:nth-of-type(10){animation-delay:.45s}
.x view:nth-of-type(11){animation-delay:.5s}
.x view:nth-of-type(12){animation-delay:.55s}
.x view:nth-of-type(13){animation-delay:.6s}
.y view {opacity:0;transform: translate(0, -7.8vw) rotate(360deg) scale(0);animation:y .5s forwards}

@keyframes y {
  30% {transform: translate(0, -3.4vw) rotate(180deg) scale(1)}
  60% {transform: translate(0, .5vw) scale(.8) rotate(0deg)}
  100% {transform: translate(0) scale(1) rotate(0deg);opacity:1}
}
