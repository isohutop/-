.fc {display: flex;align-items: center;position: relative;width: 86vw;margin:7vw 0 0 7vw;border-radius: 6vw;border:1px solid#333}
.fc .m {height: 8vw;flex:1;background-color: transparent;font-size: 3.9vw;letter-spacing: 0.75px}
.fc .l {transition: all 0.1s;box-shadow: inset 0 0 0 20px #282828;border-radius: 50%;position: relative}
.fc .l:after, .fc .l:before {display: block;content: "";position: absolute;transition: all 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55)}
.fc .l:after {width: 10px;height: 10px;background-color: #282828;border: 3px solid #f6f6f6;top: 50%;position: absolute;transform: translateY(-50%);left: 0px;right: 0;margin: auto;border-radius: 50%}
.fb .l:after {border-width: 6px;background-color: #f6f6f6}
.fc .l:before {width:4px;height: 9px;background-color: #f6f6f6;top: 64%;left: 13px;transform: rotateZ(45deg) translate(-50%, 0);transform-origin: 0 0;border-radius: 4px}
.fb .l:before {background-color: #282828;top: 50%;left: 14px;width: 3px; transform: rotateZ(45deg) translate(-50%, 11px)}
.fc .r{width:34px;height: 34px;margin: 4px 9px 4px 4px}

