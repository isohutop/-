.wh{width: 100%;position:fixed;top:0;left:0;z-index:9;overflow:hidden;background:#f5f5f5}
.wh .p{width: 100%;display:flex;align-items:center}
.wh .p>.l{font-size:3.9vw;padding:0 10px;font-weight: bolder}
.wh .p .m{font-size:3.4vw;text-align:center;flex:1;display:flex}
.wh .m>.l{width: 100%;height: 100%;border-bottom: 1px solid #f1f1f1;border-radius:3px;background: #ffffff}
.wh .q{font-size:3vw;line-height:1;padding: 9px 10px 9px 10px;}

.wh .q .m{display:flex;align-items:center;color: #444}
.wh .q .r{position:absolute;min-width:12vw;color: #939393;text-align: center}
