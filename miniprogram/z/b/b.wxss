.wv{display:flex;position:fixed;bottom:0;width:100%;height:13vw;justify-content: space-between;border-top:1rpx solid rgba(0,0,0,0.1);left:0;right:0;z-index:8;background-color:#fcfcfc;box-shadow: 0 1px 13px 0 rgba(194,194,194,0.5)}

.p{color:#5d5f68;font-size: 3.4vw;padding: 2vw 0 0.5vw;font-weight: bold}
.q{color:#7A7E82;font-size:2.8vw}

.wv>.r{display:flex;justify-content:space-between}
.wv>.r .r{width:19vw;color:#222;padding:3.9vw 0 0 0;text-align:center;height: 100%;font-weight: bolder;font-size: 3.9vw;background-color: #f5f5f5;letter-spacing: 0.8vw;border-left: 6px solid #7A7E82}

.r>.l {width: 19vw;height: 13vw;background:#f2f2f2}
.r>.l>.l{width:34px;height: 34px;margin-left: auto;margin-right: auto;margin-top: 1.3vw}

.m{margin:0 2vw;width: 13vw;height: 13vw;position: absolute}
